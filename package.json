{
  "name": "jett.taas.trips.ui",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "description": "Jett TaaS Trips UI - React TypeScript application with Vite",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "mui",
    "redux",
    "travel",
    "saas"
  ],
  "author": "Jett Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/musafirdotcom/jett.taas.trips.ui"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --mode development",
    "dev:qa": "vite --mode qa",
    "dev:uat": "vite --mode uat",
    "dev:prod": "vite --mode production",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:qa": "vite build --mode qa",
    "build:uat": "vite build --mode uat",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "preview:qa": "vite preview --mode qa",
    "preview:uat": "vite preview --mode uat",
    "preview:prod": "vite preview --mode production",
    "setup:env": "node scripts/setup-env.js",
    "setup:env:dev": "node scripts/setup-env.js development",
    "setup:env:qa": "node scripts/setup-env.js qa",
    "setup:env:uat": "node scripts/setup-env.js uat",
    "setup:env:prod": "node scripts/setup-env.js production",
    "validate:env": "node scripts/validate-env.js",
    "validate:env:dev": "node scripts/validate-env.js development",
    "validate:env:qa": "node scripts/validate-env.js qa",
    "validate:env:uat": "node scripts/validate-env.js uat",
    "validate:env:prod": "node scripts/validate-env.js production",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "bundle-report": "npm run build && npx vite-bundle-analyzer dist --mode=static"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.5.0",
    "@mui/x-data-grid": "^8.9.2",
    "@mui/x-date-pickers": "^8.10.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/dom": "^10.4.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vercel/speed-insights": "^1.2.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "formik": "^2.4.6",
    "graphql": "^16.11.0",
    "i18n": "^0.15.1",
    "i18next": "^25.3.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "js-cookie": "^3.0.5",
    "leaflet": "^1.9.4",
    "leaflet-curve": "^1.0.0",
    "leaflet-rotatedmarker": "^0.2.0",
    "moment": "^2.30.1",
    "node-forge": "^1.3.1",
    "notistack": "^3.0.2",
    "oauth-1.0a": "^2.2.6",
    "react": "^18.3.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.6.1",
    "react-leaflet": "^4.2.1",
    "react-quill": "^2.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.1",
    "react-window": "^1.8.11",
    "redux-persist": "^6.0.0",
    "sass": "^1.90.0",
    "stylis-plugin-rtl": "^2.1.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.5.3",
    "web-vitals": "^2.1.4",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20.19.0",
    "@types/node-forge": "^1.3.13",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "ts-jest": "^29.4.1",
    "util": "^0.12.5",
    "vite": "^7.0.6"
  }
}
