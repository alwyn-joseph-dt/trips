{
  "name": "jett.taas.offer.ui",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "description": "Jett TaaS Offer UI - React TypeScript application with Vite",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "mui",
    "redux",
    "travel",
    "saas"
  ],
  "author": "Jett Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/musafirdotcom/jett.taas.offer.ui"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --mode development",
    "dev:qa": "vite --mode qa",
    "dev:uat": "vite --mode uat",
    "dev:prod": "vite --mode production",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:qa": "vite build --mode qa",
    "build:uat": "vite build --mode uat",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "preview:qa": "vite preview --mode qa",
    "preview:uat": "vite preview --mode uat",
    "preview:prod": "vite preview --mode production",
    "setup:env": "node scripts/setup-env.js",
    "setup:env:dev": "node scripts/setup-env.js development",
    "setup:env:qa": "node scripts/setup-env.js qa",
    "setup:env:uat": "node scripts/setup-env.js uat",
    "setup:env:prod": "node scripts/setup-env.js production",
    "validate:env": "node scripts/validate-env.js",
    "validate:env:dev": "node scripts/validate-env.js development",
    "validate:env:qa": "node scripts/validate-env.js qa",
    "validate:env:uat": "node scripts/validate-env.js uat",
    "validate:env:prod": "node scripts/validate-env.js production",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "bundle-report": "npm run build && npx vite-bundle-analyzer dist --mode=static"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@date-io/dayjs": "^3.2.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/x-date-pickers": "^8.9.0",
    "@reduxjs/toolkit": "^2.3.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "formik": "^2.4.6",
    "i18next": "^23.16.4",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.2",
    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "node-forge": "^1.3.1",
    "notistack": "^3.0.1",
    "oauth-1.0a": "^2.2.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.0",
    "react-quill": "^2.0.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "react-window": "^1.8.11",
    "redux": "^5.0.1",
    "redux-mock-store": "^1.5.5",
    "redux-persist": "^6.0.0",
    "stylis": "^4.3.6",
    "stylis-plugin-rtl": "^2.1.1",
    "uuid": "^11.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.11.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/i18next": "^12.1.0",
    "@types/jest": "^30.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-i18next": "^7.8.3",
    "@types/redux": "^3.6.31",
    "@types/redux-mock-store": "^1.5.0",
    "@types/redux-persist": "^4.0.0",
    "@types/testing-library__user-event": "^4.1.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^21.1.0",
    "postcss": "^8.5.1",
    "postcss-prefixer": "^3.0.0",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.19",
    "vite-bundle-analyzer": "^1.1.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
